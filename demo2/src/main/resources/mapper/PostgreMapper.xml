<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sailing.transmission.mapper.PostgreMapper">

    <select id="updateVehicle" parameterType="hashmap">
       UPDATE t4_vehicle SET hpys = #{plateColor} WHERE hpzl = #{plateClass} AND hpys IS NULL
    </select>

</mapper>